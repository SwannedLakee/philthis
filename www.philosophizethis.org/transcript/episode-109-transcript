          &quot;cartBorderThickness&quot;: {
            &quot;unit&quot;: &quot;px&quot;,
            &quot;value&quot;: 1.0
          }
        },
        &quot;showButton&quot;: false,
        &quot;showCart&quot;: false,
        &quot;showAccountLogin&quot;: true,
        &quot;headerStyle&quot;: &quot;theme&quot;,
